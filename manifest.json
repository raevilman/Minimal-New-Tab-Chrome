{
	"version": "1.0",
	"name": "Minimal New Tab",
	"short_name": "Minimal New Tab",
	"description": "Chrome's new tab replacement",
	"manifest_version": 2,
	"author": "raevilman",
	"offline_enabled": true,
	"homepage_url": "https://github.com/raevilman/Minimal-New-Tab-Chrome",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"cookies"
	]
}
